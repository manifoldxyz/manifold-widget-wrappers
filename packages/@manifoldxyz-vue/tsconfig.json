{
  "compilerOptions": {
    "outDir": "dist",
    "module": "es2015",
    "sourceMap": true,
    "declaration": true,
    "importHelpers": true,
    "strict": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "jsx": "preserve",
  },
  // "include": ["src/**/*.ts", "src/**/*.vue"],
  "exclude": [
    "dist",
    "node_modules",
  ],
}
